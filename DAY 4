code 1:beginner
#include <iostream>
using namespace std;

int main() {
	int n,x,y,z;
	cin>>n;
	for(int i=1;i<=n;i++){
	    cin>>x>>y;
	    if(x>=y){
	        cout<<y<<endl;
	    }
	    else if(x<y){
	        cout<<x+(y-x)*2<<endl;
	    }   
	}
	return 0;
}

code 2:Intermediate

#include <iostream>
#include<math.h>
using namespace std;

int main() {
    int n;
    cin >> n;
    for (int i =1; i<=n; i++) {
        float l,v1,v2;
        float j,k;
        cin >> l >> v1 >> v2;
        j=ceil(l/v1);
        k=ceil(l/v2);
        if(j==k){
            cout<<-1<<endl;
        }
        else{
            cout<<j-k-1<<endl;
        }
    }

    return 0;
}

