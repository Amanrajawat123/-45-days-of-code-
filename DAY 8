CODE 1:beginner
#include <iostream>
using namespace std;

int main() {
	int n, x, y;
	cin>>n;
	for(int i=1;i<=n;i++){
	    cin>>x>>y;
	        cout<<x/(2*y)<<endl;
	}
	return 0;
}

Code 2:Intermediate

#include <iostream>
using namespace std;

int main(){
    int n, wi,pi,hi,t,bu;
    cin>>n;
    for(int i=1;i<=n;i++){
        int a=0;
        cin>>t>>bu;
        for(int j=1;j<=t;j++){
            cin>>wi>>hi>>pi;
            if(pi<=bu){
                if(a<wi*hi){
                    a=wi*hi;
                }
            }
        }
        if(a==0){
            cout<<"no tablet"<<endl;
        }
        else{
            cout<<a<<endl;
        }
    }
	return 0;
}
