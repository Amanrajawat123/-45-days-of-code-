Code : Beginner
#include <iostream>
using namespace std;

int main() {
     int n;
     cin>>n;
     for(int i=1;i<=n;i++){
         int x,y,z;
         cin>>x>>y>>z;
           if(z%y==0){
               cout<<"YES"<<endl;
           }
           else{
               cout<<"NO"<<endl;
           }
          }
     
	return 0;
}
