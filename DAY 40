Code:Beginner
#include <iostream>
using namespace std;

int main(){
    int n;
    cin>>n;
    for(int i=1;i<=n;i++){
       int x;
       cin>>x;
       if(x%5==0){
           int y=x/10+(x%10)/5;
           cout<<y<<endl;
       }
       else{
           cout<<-1<<endl;
       }
    }
    return 0;
}
