Code:Beginner
#include <iostream>
using namespace std;

int main() {
	int n;
	cin>>n;
	for(int i=1;i<=n;i++){
	    int x;
	    cin>>x;
	    if(x==1){
	        cout<<"YES"<<endl;
	    }
	    else{
	        if(x%2==0){
	            cout<<"YES"<<endl;
	        }
	        else{
	            cout<<"NO"<<endl;
	        }
	    }
	}
	return 0;
}

Code:Intermediate
#include <iostream>
#include<math.h>
using namespace std;

int main() {
	int n;
	cin>>n;
	for(int i=1;i<=n;i++){
	    float l,r,m;
	    cin>>l>>r>>m;
	    float a;
	    float b;
	    a=(m/l) ;
	    b=(m/r);
	    int c;
	    c=ceil(a)+b;
	    cout<<c<<endl;
	  
	}
	return 0;
}
