Code 1:Beginner
#include <iostream>
using namespace std;

int main() {
	int n;
	cin>>n;
	for(int i=1;i<=n;i++){
	    int x;
	    cin>>x;
	    if(x<=15){
	        if(x>0&&x<=10){
	            cout<<"Lower Double"<<endl;
	        }
	        else{
	            cout<<"Lower Single"<<endl;
	        }
	    }
	    else if(x>15&&x<=30){
	        if(x>15&&x<=25){
	            cout<<"Upper Double"<<endl;
	        }
	        else{
	            cout<<"Upper Single"<<endl;
	        }
	    }
	}
	return 0;
}
