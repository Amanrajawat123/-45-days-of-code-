Code:Beginner
  #include <iostream>
using namespace std;

int main() {
    int n;
    cin>>n;
    for(int i=1;i<=n;i++){
        int x;
        cin>>x;
        if(x%3==0){
            cout<<x%3<<endl;
        }
        else{
            int f;
            f=x%3;
            cout<<3-f<<endl;
        }
    }
	return 0;
}

Code:Intermediate
  #include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main() {
	double b;
	double a;
	char key;
	cin>>a;
	cin>>b;
	cin>>key;
	switch(key){
	    case '+':
	    cout<<a+b<<endl;
	    break;
	    case '-':
	    cout<<a-b<<endl;
	    break;
	    case '*':
	    cout<<a*b<<endl;
	    break;
	    case '/':
	    cout << fixed << setprecision(7) << a/b << endl;
	    break;
	}
	return 0;
}
