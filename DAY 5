Code 1:beginner
#include <iostream>
using namespace std;

int main() {
    int n, x;
    cin>>n;
    for(int i=1;i<=n;i++){
        cin>>x;
        if(x<=15){
            cout<<"Yes"<<endl;
        }
        else{
            cout<<"No"<<endl;
        }
    }
	return 0;
}



code 2:Intermediate

#include <iostream>
using namespace std;

int main(){
    int n,x,f;
    cin>>n;
    for(int i=1;i<=n;i++){
        cin>>x;
        f=(x/2)+1;
        cout<<f<<endl;
    }

	return 0;
}
