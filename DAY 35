Code:
#include <iostream>
using namespace std;

int main() {
	int n;
	cin>>n;
	for(int i=1;i<=n;i++){
	    int a, x, y, z;
	    cin>>a>>x>>y>>z;
	    if(a==x||a==y||a==z||a==x+y||a==y+z||a==x+z||a==x+y+z){
	        cout<<"YES"<<endl;
	    }
	    else{
	        cout<<"NO"<<endl;
	    }
	}
	return 0;
}
